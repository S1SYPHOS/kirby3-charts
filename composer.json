{
    "name": "fundevogel/kirby3-charts",
    "description": "SVG charts for Kirby v3",
    "type": "kirby-plugin",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": ["kirby3", "svg", "charts"],
    "homepage": "https://codeberg.org/fundevogel/kirby3-charts",
    "scripts": {
        "cs-dry": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix",
        "test": "phpunit --bootstrap ./tests/fixtures/bootstrap.php"
    },
    "authors": [
        {
            "name": "Kinder- und Jugendbuchhandlung Fundevogel",
            "email": "maschinenraum@fundevogel.de",
            "homepage": "https://fundevogel.de",
            "role": "Maintainer"
        },
        {
            "name": "Martin Folkers",
            "email": "hello@twobrain.io",
            "homepage": "https://twobrain.io",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "maschinenraum@fundevogel.de",
        "forum": "https://forum.getkirby.com",
        "source": "https://codeberg.org/fundevogel/kirby3-charts",
        "issues": "https://codeberg.org/fundevogel/kirby3-charts/issues"
    },
    "require": {
        "php": "^7.4|^8.0",
	    "getkirby/composer-installer": "^1.1",
        "goat1000/svggraph": "^3.13",
        "kschu91/largest-remainder-method": "^1.0"
    },
    "require-dev": {
        "getkirby/cms": "^3.5",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Fundevogel\\ChartsPlugin\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "kirby-cms-path": false
    }
}
